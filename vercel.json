{
  "buildCommand": "npx expo export -p web",
  "outputDirectory": "dist",
  "devCommand": "expo start",
  "cleanUrls": true,
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://api.tylerdipietro.com/api/:match*"
    },
    {
      "source": "/:path*",
      "destination": "/"
    },
    { "source": "/auth", "destination": "/web-build/index.html" },
    { "source": "/(.*)", "destination": "/web-build/index.html" }
  ]
}